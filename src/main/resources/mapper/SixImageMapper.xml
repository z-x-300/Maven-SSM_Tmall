<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zhangxin.tmall.dao.SixImageDao">
    <insert id="addSixImageForAdmin">
    		insert into siximage values(null,#{imageUri},#{productId})
    	</insert>
    <delete id="deleteSixImageByIdForAdmin" parameterType="int">
    		delete from siximage where id = #{id}
    	</delete>
</mapper>